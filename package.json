{
  "name": "dev-components-test",
  "version": "1.0.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/dev-components-test.umd.cjs",
  "module": "./dist/dev-components-test.js",
  "exports": {
    ".": {
      "import": "./dist/dev-components-test.js",
      "require": "./dist/dev-components-test.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "description": "dev UI Components in React",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "npm uninstall dev-components-test && rm -rf node_modules/ dist/ package-lock.json dev-components-test-*.tgz && npm install",
    "build:lib": "npm run build && npm pack && npm add dev-components-test-*.tgz",
    "build:prod": "npm run clean && npm run build:lib",
    "publish:lib":"npm uninstall dev-components-test && rm -rf node_modules/ package-lock.json && npm publish"
  },
  "author": "UT",
  "license": "MIT",
  "dependencies": {
    "dev-components-test": "file:dev-components-test-1.0.9.tgz",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5"
  }
}
